{
  "manifest_version": 3,
  "name": "Todo App Extension",
  "version": "1.0",
  "description": "A Chrome extension for managing your Todo app.",
  "permissions": ["storage", "activeTab"],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "background": {
      "service_worker": "background.js"
  }
}
